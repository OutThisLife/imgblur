{
  "name": "imgblur",
  "version": "0.1.2",
  "description": "Simple component coupled with a CLI that will lazy load your images w/ a blurry preview.",
  "repository": "https://github.com/OutThisLife/imgblur",
  "main": "dist/index.js",
  "scripts": {
    "start": "serve out/",
    "dev": "next dev",
    "build": "yarn build:clean && rollup -c",
    "build:clean": "rm -rf ./dist/*",
    "build:demo": "next build && next export",
    "build:img": "node ./bin/cli.js",
    "heroku-postbuild": "yarn build:demo",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bin": {
    "imgblur": "./bin/cli.js"
  },
  "keywords": [
    "lazyload",
    "lazy",
    "image blur"
  ],
  "author": "Talasan Nicholson",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "latest",
    "babel-core": "latest",
    "babel-eslint": "latest",
    "babel-plugin-external-helpers": "latest",
    "babel-plugin-styled-components": "latest",
    "babel-plugin-transform-object-rest-spread": "latest",
    "babel-preset-env": "latest",
    "babel-preset-flow": "latest",
    "babel-preset-react": "latest",
    "eslint": "latest",
    "eslint-plugin-flowtype": "latest",
    "eslint-plugin-flowtype-errors": "latest",
    "eslint-plugin-react": "latest",
    "flow-bin": "latest",
    "flow-copy-source": "latest",
    "next": "latest",
    "prettier": "latest",
    "rollup": "latest",
    "rollup-plugin-babel": "latest",
    "rollup-plugin-commonjs": "latest",
    "rollup-plugin-flow": "latest",
    "rollup-plugin-ignore": "latest",
    "rollup-plugin-json": "latest",
    "rollup-plugin-node-globals": "latest",
    "rollup-plugin-node-resolve": "latest",
    "rollup-plugin-replace": "latest",
    "rollup-plugin-uglify": "latest"
  },
  "dependencies": {
    "glob-fs": "^0.1.7",
    "imagemin": "^6.1.0",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^6.0.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "recompose": "^0.30.0",
    "serve": "^10.1.1",
    "sharp": "^0.21.1",
    "styled-components": "^4.1.3"
  }
}
